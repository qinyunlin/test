<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.concom.entity.dto.xunjia.ask.EpEnterpriseAskPrice">
	<!-- 实体类属性与数据库字段对应 -->
	<resultMap id="epEnterpriseAskPriceResult" type="com.concom.entity.dto.xunjia.ask.EpEnterpriseAskPrice">
	</resultMap>
	<select id="getById" resultMap="epEnterpriseAskPriceResult">
    	select * from costexpressdb.tEpEnterpriseAskPrice where id = #{_parameter}
    </select>
	<select id="queryPage"  resultMap="epEnterpriseAskPriceResult">
     	<!-- SELECT id, subcid, proName, name, spec, unit, price, supplyName, recoveryTime, fid, fname, state, picPath FROM costexpressdb.tEpEnterpriseAskPrice -->
     	SELECT * FROM costexpressdb.tEpEnterpriseAskPrice
     	<where>
     	    1=1
     	    <if test="params.fname != null and params.fname != ''"> AND fname LIKE '%${params.fname}%'</if>
     	    <if test="params.name != null and params.name != ''"> AND name LIKE '%${params.name}%'</if>
     	    <if test="params.spec != null and params.spec != ''"> AND spec LIKE '%${params.spec}%'</if>
     	    <if test="params.supplyName != null and params.supplyName != ''"> AND supplyName LIKE '%${params.supplyName}%'</if>
     	    <if test="params.fid != null and params.fid != ''"> AND fid = #{params.fid}</if>
     	</where>
     	ORDER BY id DESC
    </select>
    <delete id="delete" parameterType="com.concom.entity.dto.xunjia.ask.EpEnterpriseAskPrice">
        delete from costexpressdb.tEpEnterpriseAskPrice
        <where>
            1 = 1 
            <if test="id != null and id != ''">
                and id = ${id}
            </if>
        </where>
    </delete>
</mapper>