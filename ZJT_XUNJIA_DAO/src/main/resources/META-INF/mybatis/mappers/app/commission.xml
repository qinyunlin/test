<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.concom.entity.dto.xunjia.app.commission.Commission">
	
	
    <select id="queryList" resultType="com.concom.entity.dto.xunjia.app.commission.Commission">
     		select * from youcai.tCommission 
    </select>

    <update id="update" parameterType="com.concom.entity.dto.xunjia.app.commission.Commission">
        update youcai.tCommission 
          <set>
          	<if test="commission !=null and commission !=''">
          	    commission=#{commission}
          	</if>
          
          </set>
         where id=#{id}
    </update>
    
    <insert id="insert" parameterType="com.concom.entity.dto.xunjia.app.commission.Commission">
        insert into youcai.tCommission (commission) values (#{commission})
    </insert>
</mapper>