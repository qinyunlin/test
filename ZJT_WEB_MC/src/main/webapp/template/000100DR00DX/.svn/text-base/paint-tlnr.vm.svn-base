<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
#set($content=$dataInfo.info)
  <head> <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    
    <title>$!content.title -网材网资讯平台</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<meta name="keywords" content="$!content.tags" />
 <meta name="description" content="" />
<link rel="stylesheet" type="text/css" href="$wcw_common_site/css/demand/zxnr.css">
<link rel="stylesheet" type="text/css" href="$wcw_common_site/css/common/zxcg.css">
<script type="text/javascript" src="$wcw_common_site/js/common/jyeeda.js"></script>
	
			<script language="javascript"
					src="$wcw_common_site/js/common/msg_zhcn.js"></script>
			<script language="javascript"
					src="$wcw_common_site/js/common/basic.js"></script>
			<script language="javascript"
					src="$wcw_common_site/js/common/ConCom.js"></script>
			<script language="javascript" src="$wcw_common_site/js/common/common.js"></script>
			<script language="javascript" src="$wcw_common_site/js/common/dataStore.js" ></script>
			    <script type="text/javascript" src="$wcw_common_site/js/project/project_info.js"></script>
  </head>
     
  <body>
     #parse($wcw_header)
	 #parse($hypd_nav)
	 <div class="main">
        <div class="ccwcw_zxnr_center2">
			       	
					$!map.clear()
					$!map.put("isHot", "0")
					$!map.put("isTop", "1")
					$!map.put("tpath", "/20/2012/201203/20120325/")
					#set($list = $infoService.searchInfoContentSummarys(1, 1,$!map, true))
					#foreach($obj in $list)
					   <a rel="external nofollow" href="$!obj.getUrl()" target="_blank">
			        	<img alt="$!obj.getTitle()" width="950px" height="80px"  src="$!fileUtil.getWcwFileUrl($!obj.getPicPath())">
			   		</a>
			      #end
		        </div>
		        
		        
		<div id="crumb">
				<ul>
			    	<li class="crumb_hp"><a href="http://www.ccwcw.com">网材网</a></li>
			        <li><a href="http://www.ccwcw.com/hypd">行业频道</a></li>
			        <li><a href="http://www.ccwcw.com/hypd/paint-index.html">涂料栏目</a></li>
			        <li><a href="$templateUtil.getInfoContentList($!content.tpath)">$content.typename</a></li>
			        <li>正文</li>
			    </ul>
		  </div>
		        
		     
       <div class="ccwcw_zxnr_center3">
         <div class="ccwcw_zxnr_center3_left" id="info_body">
            <div class="title_banner" style="width:662px;">
                <div class="title_banner_left"></div>
                <div class="title_banner_center" style="width:656px;">
                    <div class="title_icon"></div>
                    <div class="title_text" id="tname">$!content.typename</div>
                </div>
                <div class="title_banner_right"></div>
            </div> 
            <div class="ccwcw_zxnr_center3_left_bottom">
           
			     <h6 id="title">
								$!content.title
				</h6>
				<div class="ccwcw_zxnr_xx">
					<span><a href="http://www.ccwcw.com/">http://www.ccwcw.com/</a></span>&nbsp;&nbsp;&nbsp;&nbsp;
					<span id="addTime">$!content.addTime</span>&nbsp; 来源：
					<span id="source">$!content.source</span>
				</div>
				<div id="content" class="ccwcw_zxnr_xx_context">
				  $!content.content
        		</div>
        		
        		
        		<!-- 相关链接 -->
              #if($content.tags != "null" || $content.tags != "")
	            	$!map.clear()
	            	$!map.put("tpath", "/20/")
					$!map.put("tags", $content.tags)
					#set($list = $infoService.searchInfoContentWidthExCoulmn(10, 1,"id != $content.id", $!map, true))
					#if($list.size() != 0)
                     <div class="xg_link_box">
			       		<h1 class="xg_link_box_title">相关链接：</h1>
			            <ul>
							#foreach($obj in $list)
								#if($velocityCount%2!=0)
				            	<li class="m_r_20"><a href="$templateUtil.getWcwInfoContentUrl($!obj.getId(),$!obj.getTpath(),$!obj.getAddTime())" target="_blank" title="$obj.title">$obj.subTitle</a></li>
				            	#else
				                <li><a href="$templateUtil.getWcwInfoContentUrl($!obj.getId(),$!obj.getTpath(),$!obj.getAddTime())" target="_blank" title="$obj.title">$obj.subTitle</a></li>
				                #end
			                #end
		            #end
			            </ul>
					 </div>
			#end
			</div>
           
         </div>
		        <div class="ccwcw_zxnr_center3_right">
			      <div id="jghq">
			      
					<div class="title_banner" style="width:277px;">
						<div class="title_banner_left"></div>
						<div class="title_banner_center" style="width:271px;">
							<div class="title_icon"></div>
							<a  target="_blank" class="black_a" href="$templateUtil.getInfoContentList('/20/2003/200305/')"><div class="title_text">价格行情</div></a>
						</div>
						<div class="title_banner_right"></div>
					</div>
					<div class="jqhq_container">
						<div class="jghq_div">
						
							   $!map.clear()
				         $!map.put("tpath", "/20/2003/200305/")
				         #set($list = $infoService.searchInfoContentSummarys(8, 1,$!map, true))
				         #foreach($obj in $list)           
							<ul>
								<li class="xmsqs_orange_p2"></li>
								<li class="jghq_text">
									<a  target="_blank" class="black_a" href="$templateUtil.getWcwInfoContentUrl($!obj.getId(),$!obj.getTpath(),$!obj.getAddTime())" style="font-size:14px;">$!parameterUtil.truncString($!obj.getSubTitle(),38,"")</a>
								</li>
							</ul>
							
						#end
						</div>
			       </div>
			     
		       </div>
	          <div class="ccwcw_zxnr_center3_right_div2">
	                  
          				
							 $!map.clear()
							 $!map.put("isHot", "0")
                                                         $!map.put("isTop", "1")
							 $!map.put("tpath", "/20/2012/201203/20120326/")
							 #set( $list = $infoService.searchInfoContentSummarys(1, 1, $!map, true))
							 #foreach($obj in $list)
	                   <a rel="external nofollow" href="$!obj.getUrl() " target="_blank">
	                   		<img alt="$!obj.getTitle()" width="277px" height="110px"  src="$!fileUtil.getWcwFileUrl($!obj.getPicPath())"/>
	                   </a>
	             #end
	          </div>
	         #parse($wcw_tlzx)
	              
	          #parse($wcw_aljs) 
	          #parse($wcw_bzfg)
           </div>
       </div>
   </div>
          #parse($wcw_footer) 
          
 
  </body>

</html>
