								    		</div>
								    	</div>
			    	#end
		        </div>
		    
		    	
		    	  <div id="page">
#parse($shop_page_dir)
				  </div>
		    </div>
		    <div class="clearing"></div>
	    </div>
     </div>
     #parse($wcw_footer)

