	    						<li class="orange_p"></li>
	    						<li class="xcbd_list_text"><a  target="_blank" class="black_a" title="$!obj.getSubTitle()" href="$templateUtil.getWcwInfoContentUrl($!obj.getId(),$!obj.getTpath(),$!obj.getAddTime())" style="font-size:14px;">$!parameterUtil.truncString($!obj.getSubTitle(),38,"" )</a></li>
	    					</ul>
                                                      #end
				                    
	    				</div>
	    		
	    		   </div>
	    		       
				 </div>	
	    	  </div>
	    	</div>
	    
