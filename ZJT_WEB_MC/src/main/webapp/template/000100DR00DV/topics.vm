                  <div class="c_t_top_title_right"></div>
               </div>
               <div class="c_t_top_content" id="introduction">
                     $!content.introduction
               </div>
               <div class="c_t_top_datum">
                 <div class="c_t_top_l">
	                  <div class="c_t_time">开始时间：<span style="color:#D76B00;" id="startTime">$!content.startTime</span></div>
	                  <div class="c_t_addr">举办地址：<span style="color:#D76B00;" id="holdAddr">$!content.holdAddr</span></div>
                 </div>
                 
                 <div class="c_t_top_l" style="margin-left:34px;">
	                  <div class="c_t_time">联系电话：<span style="color:#D76B00;"  id="phone">$!content.phone</span></div>
	                  <div class="c_t_addr">主办单位：<span style="color:#D76B00;" id="holdUnit">$!content.holdUnit</span></div>
                 </div>
                 
                 <div class="c_t_top_l" style="margin-left:34px;">
	                  <div class="c_t_time">采购范围：<span style="color:#D76B00;"id="purchaseRange">
                                         <a  target="_blank" class="black_a" title='$!parameterUtil.purchasingRange($!content.purchaseRange)'>$!parameterUtil.purchasingRange($!content.purchaseRange)</a></span></div>    
                 </div>
                 
                 <div class="c_t_top_l" style="margin-left:30px;">
	                  <div class="c_t_bm" onclick="regApplay($!content.id)"></div>   
                 </div>
               
               </div>
             </div>
             <div class="c_t_center">
                <div class="c_t_chyq"></div>
                <div class="c_t_ygs" align="left">
                  <div class="c_t_ygs_left">
                     <div class="c_t_gys_title">供应商:</div>
                     <div class="c_t_gys_name" id="supplierRequirement">
                               $!content.supplierRequirement
                     </div>
                  </div>
                  
                  <div class="c_t_ygs_left">
                     <div class="c_t_gys_title">采购方:</div>
                     <div class="c_t_gys_name" id="purchaserRequirement">         
                        $!content.purchaserRequirement
                     </div>
                  </div>
                </div>

             </div>
             <div class="c_t_center">
                <div class="c_t_chdw"></div>
                <div class="c_t_cg" align="left">
                  <div class="c_t_cgdw_left">
                     <div class="c_t_gys_title" style="width:58px;">采购单位:</div>
                     <div class="c_t_cgdw_name" style="width:835px;">
                       <ul id="unit_listbody">
							
	   		#foreach($obj in $sunshinePurchasesList)
                             $!parameterUtil.guestUnitSplit($!obj.guestUnit)                         #end	   				
		      </ul>
                     </div>
                  </div>
                  
                 
                </div>
<div class="clearing"></div>
             </div>
