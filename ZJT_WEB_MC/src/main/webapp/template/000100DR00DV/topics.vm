			                      <ul id="list_body">
			                         #foreach($obj in $registrationSuppliersList)
                                                         <li><a   target="_blank" class="black_a" href="http://www.ccwcw.com/shop/$!obj.eid">$!obj.ename</a></li>
                                                 #end
			                      </ul>
			                   </div>
			                </div>
			               
			                <div class="bmgys" style="float:right; margin-left:0px;" align="left">
			                   <div class="bmgys_title">入围供应商:</div>
			                   <div class="bmgys_name">
			                      <ul id="short_listbody">
			                         #foreach($obj in $shortlistedSuppliersList)
                                                          <li><a class="black_a"  target="_blank"  href="http://www.ccwcw.com/shop/$!obj.eid">$!obj.ename</a></li>
                                                 #end
			                      </ul>
			                   </div>
			                </div>
			               
			             </div>
