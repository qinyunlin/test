	    			<div class="ygcg_r_title_r"></div>
	    		</div>
	    		<div class="xcbd_container">
	    		   
	                            $!map.clear()
				    $!map.put("tpath", "/20/2004/200406/")
				    $!map.put("isTop", "1")								
				    #set($list =$infoService.searchInfoContentSummarys(1, 1, $!map, true)) 
				    #foreach($obj in $list)
                                           <div class="xcbd_container_l">
	    				<div class="xcbd_img_div">
	    					<div class="xcbd_img">
	    						<a  target="_blank" href="$templateUtil.getWcwInfoContentUrl($!obj.getId(),$!obj.getTpath(),$!obj.getAddTime())"><img width="207px" alt="$!obj.getSubTitle()" height="154px" src="$!fileUtil.getWcwFileUrl($!obj.getPicPath())"/></a>
	    						<div class="xcbd_img_date">$!obj.getAddTime()</div>
	    					</div>
	    				</div>
	    			</div>
	    			<div class="xcbd_container_r">
	    				<div class="xcbd_title">
	    					<a class="black_a"  target="_blank" href="$templateUtil.getWcwInfoContentUrl($!obj.getId(),$!obj.getTpath(),$!obj.getAddTime())" title="$!obj.getSubTitle()" style="font-size:14px;">$!parameterUtil.truncString($!obj.getSubTitle(),38,"" )</a>
	    				</div>
	    				<div class="xcbd_text">    
	    				   $!obj.getSummary() 
	    				</div>                  
                                    #end
				                    	
				                    	 
                		
	    			
					
	    				<div class="xcbd_list">
	    					
						     $!map.clear()
				                     $!map.put("tpath", "/20/2004/200406/")
				                     $!map.put("isTop", "0")
						     $!map.put("isHot", "0")
				                    #set($list =$infoService.searchInfoContentSummarys(3, 1, $!map, true)) 
                                                       #foreach($obj in $list)
