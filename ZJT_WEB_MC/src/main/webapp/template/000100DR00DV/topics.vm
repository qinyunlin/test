	    				<div class="mtbd_container_img">
	    					<a class="black_a"  target="_blank" href="$templateUtil.getWcwInfoContentUrl($!obj.getId(),$!obj.getTpath(),$!obj.getAddTime())"><img width="100px" alt="$!obj.getSubTitle()" height="84px" src="$!fileUtil.getWcwFileUrl($!obj.getPicPath()) "/></a>
	    				</div>
	    				<div class="mtbd_container_content">
	    					<div class="mtbd_container_title">
	    						<a class="black_a"  target="_blank" href="$templateUtil.getWcwInfoContentUrl($!obj.getId(),$!obj.getTpath(),$!obj.getAddTime())" title="$!obj.getSubTitle()" style="font-size:14px;">$!parameterUtil.truncString( $!obj.getSubTitle(),20,"" )</a>
	    					</div>
	    					<div class="mtbd_container_text">$!obj.getSummary() </div>
	    				</div>
	    				
	    			</div>
	    				#end
	    		</div>
