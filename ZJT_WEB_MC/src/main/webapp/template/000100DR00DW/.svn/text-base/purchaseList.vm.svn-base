<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<html>
  <head> <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <title>建材求购列表 – 网材网 – 中国最大的建材求购网</title>
 <meta name="keywords" content="建筑材料，建材信息，建材材料，建材求购" />
 <meta name="description" content="网材网建材求购列表板块是中国最大的建材求购网。是由大量的建材求购方面向全国建材供应商发布的建材求购信息。/>


    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<link rel="stylesheet" type="text/css" href="$wcw_common_site/css/tradeChannel/purchaseList.css"/>
	<link rel="stylesheet" type="text/css" href="$wcw_common_site/css/common/rightMode.css"/>
	<link rel="stylesheet" type="text/css" href="$wcw_common_site/css/common/zxcg.css"/>
	<script type="text/javascript" src="$wcw_common_site/js/common/jyeeda.js"></script> 
	<script type="text/javascript" src="$wcw_common_site/js/common/dataStore.js"></script>
	<script type="text/javascript" src="$wcw_common_site/js/common/pageTool.js"></script>
	<script type="text/javascript" src="$wcw_common_site/js/common/basic.js"></script>
	<script type="text/javascript" src="$wcw_common_site/js/common/dataCheck.js"></script>
	<script type="text/javascript" src="http://c2.zjtcn.com/js/zjt/commonJS/cg_cid_db.js"></script>
	<script type="text/javascript" src="http://c2.zjtcn.com/js/common/cg_cid_util.js"></script>
	<script type="text/javascript" src="$wcw_common_site/js/common/ConCom.js"></script> 
	<script type="text/javascript" src="$wcw_common_site/js/common/common.js"></script>
	
  </head>
  

  
  <body>
   <!-- 头部 -->
   #parse($wcw_header)
    
	<div class="main">
	#parse($hypd_nav)
	
	<!-- 行业分类链接容器 -->
	<div class="type_container">
		<div class="sub_style_box">
	<h2>
    	<span class="arrow_i"><a href="javascript:void(0);">全部采购分类</a></span><a id="cgCidName" href=""></a>
    </h2>
    <ul id="cgCidList" class="clearfix">
    	
    </ul>
</div>
<dl class="sel_city_box clearfix">
	<dt>地区：</dt>
    <dd>
       <div style="text-align:left;">
        <ul id="province_div" class="nava clearfix">
            
         
        </ul>
        </div>
        <div class="dim" style="display:none;" id="city_div">
           
        </div>
    </dd>
</dl>
		<div class="clearing"></div>
	</div>
	<div class="clearing"></div>
	<!-- 主体 -->
	<div id="main">
		<div class="main_l">
			<div class="title_banner" style="width:665px;">
				<div class="title_banner_left"></div>
				<div class="title_banner_center" style="width:659px;">
					<div class="title_icon"></div>
					<div class="title_text">求购列表</div>
				</div>
				<div class="title_banner_right"></div>
			</div>
			<div class="searchlist_container">
				<div class="searchlist_div">
				
				
					<div id="listbody"></div>
					<div id='page'></div>
					
					<!-- template -->
					<div id="templistbody" style="display:none;">
						<div class="searchlist_content">
							<div class="list_num">@{rownum}</div>
							<div class="list_img">
								<img title="@{title}" width="80px" height="80px" src="@{photo|changeLogo}" />
							</div>
							<div class="list_container">
								<div class="list_container_b">
									<ul style="margin-bottom:5px;">
										<li class="type_name" title="@{title}"><a  target="_blank" class="blue_a" href="/gcxm/details.html?id=@{id}" style="font-size:14px;">@{title}</a></li>
										<li class="com_area" style="float:right">发布时间：<span>@{startTime|changeTime}</span></li>
									</ul>
									<ul>
										<li>规格型号：</li>
										<li>@{spec}</li>
									</ul>
									<ul>
										<li>数量：</li>
										<li class="li_class">@{amount}@{unit}</li>
										<li>品牌：</li>
										<li>@{brand}</li>
									</ul>
									<ul>
										<li>地址：</li>
										<li>@{addr}&nbsp;&nbsp;求购单位：@{ename}</li>
										<li style="float:right;">@{endTime|getValidDay}</li>
										<li style="float:right;">结束时间：</li>
									</ul>
								</div>
							</div>
						</div>		
					</div>
					

				</div>
			</div>
		</div>
		<div class="main_r">
			<!-- 最新求购模块 -->
			#parse($wcw_zxcg)
			
			<div class="img_div1">
				 
							$!map.clear()
							$!map.put("isHot", "0")
							$!map.put("isTop", "1")
							$!map.put("tpath", "/20/2012/201204/20120411/")
							#set($list = $infoService.searchInfoContentSummarys(1, 1,$!map, true))
							 #foreach($obj in $list)
	                   <a rel="external nofollow" href="$!obj.getUrl()" target="_blank"><img alt="$!obj.getTitle()" width="277px" height="110px" src="$!fileUtil.getWcwFileUrl($!obj.getPicPath())"/></a>
	                #end
			</div>
			
			<!-- 项目进度模块 -->
			#parse($wcw_xmjd)
			
			<div class="img_div2">
				
							$!map.clear()
							$!map.put("isHot", "0")
							$!map.put("isTop", "1")
							$!map.put("tpath", "/20/2012/201204/20120412/")

							#set($list = $infoService.searchInfoContentSummarys(1, 1,$!map, true))
							 #foreach($obj in $list)
	                   <a rel="external nofollow" href="$!obj.getUrl()" target="_blank"><img alt="$!obj.getTitle()" width="277px" height="110px" src="$!fileUtil.getWcwFileUrl($!obj.getPicPath())"/></a>
	                #end
	                   
	                 
			</div>
			
			<!--地区项目发布模块 -->
			#parse($wcw_dfxmfb)
		</div>
		<div class="clearing"></div>
	</div>
	</div>
    <!-- 尾部 -->
    #parse($wcw_footer)
    
    <script type="text/javascript" src="$wcw_common_site/js/tradeChannel/purchaseList.js"></script>
  </body>
</html>

