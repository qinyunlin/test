<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
  <head> <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    
    <title>行业频道</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<link rel="stylesheet" type="text/css" href="$wcw_common_site/css/common/zxcg.css">
        <link rel="stylesheet" type="text/css" href="$wcw_common_site/css/common/common.css">
		
  </head>
  
  <body>

<div class="cggs_contect">
	<div class="title_banner" style="width: 277px;">
		<div class="title_banner_left"></div>
		<div class="title_banner_center" style="width: 271px;">
			<div class="title_icon"></div>
			<a  target="_blank" class="black_a" href="$templateUtil.getInfoContentList('/20/2007/200704/')"><div class="title_text">
				成功故事
			</div></a>
			<a class="blue2_a gengduo" href="$templateUtil.getInfoContentList('/20/2007/200704/')"  target="_blank">更多＞＞</a>
		</div>
		<div class="title_banner_right"></div>
	</div>
	<div class="cggs_div">
		
			$!map.clear()
                    	$!map.put("tpath", "/20/2007/200704/")
                    	$!map.put("isTop","1")
                    	#set($list = $infoService.searchInfoContentSummarys(1, 1,  $!map, true)) 
                        #foreach($obj in $list)
		<div class="cggs_img">
			<a  target="_blank" href="/hypd/zxnr.html?id=$!obj.getId()">
			<img width="270px" height="130px"
				src="$!fileUtil.getWcwFileUrl($!obj.getPicPath())" /></a>
		</div>
		<div class="cggs_title">
		   #if($!obj.getIssueType()=="1")
			<a  target="_blank" class="black_a" href="$templateUtil.getWcwInfoContentUrl($!obj.getId(),$!obj.getTpath(),$!obj.getAddTime())"
			 style="font-size: 14px;">$!parameterUtil.truncString($!obj.getSubTitle(),30,"")</a>
		   #else
		    <a  target="_blank" class="black_a" href="$!obj.getUrl()"
			 style="font-size: 14px;">$!parameterUtil.truncString($!obj.getSubTitle(),30,"")</a>
		   #end
		</div>
		<div class="cggs_text">
			$!parameterUtil.truncString($!obj.getSummary(),155,"")【
			#if($!obj.getIssueType()=="1")
			<a  target="_blank" class="blue_a" href="$templateUtil.getWcwInfoContentUrl($!obj.getId(),$!obj.getTpath(),$!obj.getAddTime())">详细</a>
			#else
			 <a  target="_blank" class="blue_a" href="$!obj.getUrl()">详细</a>
			#end
			】
		</div>
		#end
	</div>
</div>

</body>
</html>
