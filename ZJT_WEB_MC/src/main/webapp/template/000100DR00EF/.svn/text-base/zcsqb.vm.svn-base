<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head> <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		<title>申请云采购</title>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <link type="text/css" rel="stylesheet" href="$wcw_common_site/css/common/reset.css" />
		<link type="text/css" rel="stylesheet" href="$wcw_common_site/css/common/registration_common.css" />
		<link type="text/css" rel="stylesheet" href="$wcw_common_site/css/common/cgsjs_common.css"/>
		<link rel="stylesheet" type="text/css" href="http://c1.ccwcw.com/js/plugins/formValidator3.3/style/validator.css" />
	</head>

	<body>
     #parse($wcw_header)
<div id="top_box">
	<div class="top_banner">
    	<div class="top_info">
        	<a href="#" onclick="applyForPurchasingSteward()" class="btn_zcsq" title="注册申请云采购">注册申请云采购</a>
            <a href="#" class="btn_zxzx" onclick="javascript:window.open('http://b.qq.com/webc.htm?new=0&sid=4008889639&eid=218808P8z8p8y8y8z8z8y&o=www.zjtcn.com&q=7&ref='+document.location, '_blank', 'height=544, width=644,toolbar=no,scrollbars=no,menubar=no,status=no');"  title="在线咨询">在线咨询</a>
        </div>
    </div>
</div>



<div id="contanier01" style="text-align:left;" >
	<span class="shubiao_bg"></span>
    <h1 class="title_bg title_02"></h1>
    <div class="bg_01">
    	<div class="reg_info_box">
        	<h1 class="til_line"><span>注册信息</span></h1>
            
            <div class="con_form">
            	<div class="reg_info_01">
                    <div class="l"><em>*</em>会员名</div>
                        <div class="c"><input id="memberID" type="text" onblur="valUserId()"
										method="isUserName" class="txt "  min_length="6" /></div>
                        <div id="tip">
                        	<span  id="memberIDTip"></span>
                        	
                        </div>
                </div>
                <div class="reg_info_01">
                   <div class="l"><em>*</em>密码</div>
                        <div class="c"><input type="password" id="pwd" method="isPassWord" min_length="6"
										maxlength="16" class="txt "  /></div>
                        <div >
                        <span  id="pwdTip"></span>
                            
                        </div>
                </div>
                <div class="reg_info_01">
                     <div class="l"><em>*</em>再次输入密码</div>
                        <div class="c"><input type="password" id="reguseryzm" method="isPassWord"maxlength="16" onblur="valConfirmPwd()" class="txt" /></div>
                        <div >
                        	<span id="reguseryzmTip"></span>
                        	
                        </div>
                </div>
                <div class="reg_info_01">
                    <div class="l"><em>*</em>电子邮箱</div>
                        <div class="c"><input type="text" id="email"  max_length="50" onblur="valUserEmail()" class="txt" /></div>
                        <div >
                        	 	<span  id="emailTip"></span>
                        	
                        </div>
                </div>
            </div>
            
            <h1 class="til_line"><span>企业信息</span></h1>
            
            <div class="con_form">
            	<div class="reg_info_01">
                    <div class="l"><em>*</em>企业名称：</div>
                    <div class="c"><input id="ename" type="text" class="txt"/></div>
                    <div>
                       <span  id="enameTip"></span>
                        	
                    </div>
                </div>
                <div class="reg_info_01">
                     <div class="l"><em>*</em>所在地区</div>
                        <div class="c">
                        <select id="province" name="province" class="country" onchange="change();"></select>-
                                    <select id="city" name="city" class="country"></select>
                        </div>
                        <div >
                        	<span class="onCorrect"></span>
                        </div>
                </div>
                <div class="reg_info_01">
                    <div class="l"><em>*</em>联系人：</div>
                    <div class="c"><input id="linkName" type="text" class="txt" /></div>
                    <div>
                       <span  id="linkNameTip"></span>
                        	
                    </div>
                </div>
                <div class="reg_info_01">
                    <div class="l"><em>*</em>联系方式：</div>
                    <div class="c"><input id="phone" type="text" class="txt" /></div>
                    <div >
                        	<span id="phoneTip"></span>
                    </div>
                </div>
                <div class="reg_info_01">
                     <div class="l"><em>*</em>验证码</div>
                        <div class="c">
                        <input name="verifyCode" type="text" id="verifyCode" class="txt w_80 f_l" style="width:158px;" />
                        <div class="yzm_box f_l">
                        	<img id="imgCode" src="/authImg" />
                        </div>
                        <span style="position:relative;top:7px;">
                        	<a href="#" onclick="refreshCode()" class="link_gray text_dec_underline">看不清,换一个</a>
                        </span>
                        </div>
                        <div class="r"></div>
                </div>
                <div class="reg_info_01">
                    <div class="l"></div>
                        <div class="c"><input name="clausecheck" type="checkbox" 
										checked id="clausecheck" /><a href="service_explain.html" class="link_gray text_dec_underline">我已经看过并同意《网材网服务条款》</a></div>
                        <div class="r"></div>
                </div>
                <div class="reg_info_01">
                     <div class="l"></div>
                        <div class="c"><a class="btn_reg" onclick="submitInfo()" href="#">立即提交申请</a></div>
                        <div class="r"></div>
                </div>
            </div>
        </div>
    </div>
</div>

#parse($wcw_footer)
<script type="text/javascript" src="$wcw_common_site/js/common/msg_zhcn.js"></script>
		<script type="text/javascript" src="$wcw_common_site/js/common/dataStore.js"></script>
		<script type="text/javascript" src="$wcw_common_site/js/common/dataCheck.js"></script>
		<script type="text/javascript" src="$common_site/js/zjt/commonJS/zhcn_db.js"></script>
		<script language="javascript"  src="$wcw_common_site/js/common/zhcn_select.js"></script>
		<script type="text/javascript" src="$wcw_common_site/js/common/common.js"></script>
		
		<script type="text/javascript"
			src="$wcw_common_site/js/common/login_register_common.js"></script>
	<script type="text/javascript" src="http://c1.ccwcw.com/js/plugins/formValidator3.3/formValidator_min.js"></script>
        <script type="text/javascript" src="http://c1.ccwcw.com/js/plugins/formValidator3.3/formValidatorRegex.js"></script>
<script type="text/javascript" src="$wcw_common_site/js/common/zcsqb.js"></script>
	</body>
</html>
