<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
## 获取商铺信息
#set($epShop=$epShopService.getEpShopByEID($dataInfo.eid))
## 获取分类信息
#set($productClassifyList=$epShopService.getProductClassify($dataInfo.eid))
## 获取企业图片信息
#set($epAttachList=$epShopService.getEpAttachList(20, 1, "2", $dataInfo.eid))
<html>
  <head> <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<link rel="stylesheet" type="text/css" href="$!{c1wcw_dir}/css/common/reset.css" />
<link rel="stylesheet" type="text/css" href="$!{c1wcw_dir}/css/common/topbar_common.css" />
<link rel="stylesheet" type="text/css" href="$!{c1wcw_dir}/css/shop/shops03_common.css" />
<link rel="stylesheet" type="text/css" href="$!{c1wcw_dir}/css/shop/shops03_about.css" />

	<link type="text/css" rel="stylesheet" href="$!{c1wcw_dir}/css/shop/shops03_index.css" />
	<script type="text/javascript" src="$!{c1wcw_dir}/js/common/jyeeda.js"></script> 
	<script type="text/javascript" src="$!{c1wcw_dir}/js/plugins/jqueryTools.js"></script> 
    <script type="text/javascript" src="$!{c1wcw_dir}/js/common/dataStore.js"></script>
	<script type="text/javascript" src="$!{c1wcw_dir}/js/common/pageTool.js"></script>
	<script type="text/javascript" src="$!{c1wcw_dir}/js/common/basic.js"></script>
	<script type="text/javascript" src="$!{c1wcw_dir}/js/common/dataCheck.js"></script>
	<script type="text/javascript" src="$!{c1wcw_dir}/js/common/ConCom.js"></script>
	<script type="text/javascript" src="$!{c1wcw_dir}/js/common/jquery.cookie.js"></script>
	<script type="text/javascript" src="$!{c1wcw_dir}/js/common/shop_type_db.js"></script>
		<script type="text/javascript" src="$!{c1wcw_dir}/js/common/common.js"></script>
	
	<title>$!{epShop.getName()}_企业介绍_网材网</title>

  </head>
  
 <body>
<!--header star	-->
    #parse($shop_vip_header_dir)
<!--	header end	-->


<!--	contanier star	-->
<div class="contanier">
	#parse($shop_vip_left_dir)
  <div class="right">
    
    	<!--	moudel_box_right star	-->
        <div class="moudel_box_right">
       	  <h1 class="title_bg">企业介绍</h1>
            
            <!--	qyjs_box_01 star	-->
            <div class="qyjs_box_01  clearfix">
                <img src="$!{ftp_pic}$!{epShop.getPicPath()}" />
                 $!{epShop.getDiscription()}
            </div>
            <!--	qyjs_box_01 end	-->
            
            <div class="qyjs_img_box">
                <div class="mall_big_pic" style="display:none;">
            	<img  id="pic_max"  src="../images/about_small_img_320x320.jpg" />
            	</div>
	       	    	 	
            	<a class="mall_prev prev" href="javascript:;"></a>

                    
                     <div class="demand-scrcont">
                        <div id="list" >
		                  #foreach ($epAttach in $epAttachList )
							<a><img id="pic_$velocityCount" width="98" height="98" src="$!{ftp_pic}$!{epAttach.path}" title="$!{epAttach.filename}" /></a>
						  #end
		                </div>
		              </div>
                    <a class="mall_next next" href="javascript:;"></a>
            	
            </div>
            
        </div>
        <!--	moudel_box_right end	-->
        
        
        <div class="moudel_box_right">
            <h1 class="title_bg">详细信息</h1>
            <div class="xx_info_box">
                <table>
                	<tr>
                    	<th width="15%">公司名称：</th>
                        <td width="85%">$!{epShop.getName()}</td>
                    </tr>
                    <tr>
                    	<th>所属行业：</th>
                        <td>$!{epShop.getCname()}</td>
                    </tr>
                    <tr>
                    	<th>所属地区：</th>
                        <td>$!{epShop.getProvince()}$!{epShop.getCity()}</td>
                    </tr>
                    <tr>
                    	<th>主营产品：</th>
                        <td>$!{epShop.getMainMaterial()}</td>
                    </tr>
                    <tr>
                    	<th>销售区域：</th>
                        <td>$!{epShop.getSaleArea()}</td>
                    </tr>
                    <tr>
                    	<th>企业地址：</th>
                        <td>$!{epShop.getAddr()}</td>
                    </tr>
                    <tr>
                    	<th>联系人：</th>
                        <td>$!{epShop.getContact()}</td>
                    </tr>
                    <tr>
                    	<th>联系手机：</th>
                        <td>$!{epShop.getMobile()}</td>
                    </tr>
                    <tr>
                    	<th>联系电话：</th>
                        <td>$!{epShop.getPhone()}</td>
                    </tr>
                    <tr>
                    	<th>传真号码：</th>
                        <td>$!{epShop.getFax()}</td>
                    </tr>
                    <tr>
                    	<th>企业网址：</th>
                        <td>$!{epShop.getHomePage()}</td>
                    </tr>
                </table>
            </div>
        </div>
        
    </div>
    <div class="clearfix"></div>
</div>
<!--	contanier end	-->


<!--	footer star	-->
    #parse($shop_vip_footer_dir)
     
       <script type="text/javascript" src="$!{c1wcw_dir}/js/shop/shop_image_html.js"></script>
