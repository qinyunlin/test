<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
 <title>供应商报价_($dataInfo.province)各地建材厂商、供应商材料报价_($dataInfo.province)造价通</title>
    <meta name="description"
        content="50000多家优质材料供应商，定期为您提供最新的材料厂商报价，各类主材、辅材价格、设备机械租赁价格、厂商合同价、建议价每月更新，累计报价材料达数千万。"/>
        <meta name="keywords"	
            content="厂商报价，建材价格，材料报价，设备租赁价，建议价"/>
<link rel="icon" href="/favicon.ico" type="image/x-icon" />
</head>
<body>
#parse($header_dir)
<!--	container	-->
<div class="container">
	<!--	面包屑	-->
	<div class="breadCrumb" id="fac_dh">
    </div>
    <!--	面包屑	-->
    <div class="layout-box m-t-5 clearfix">
    	<!--	左侧树	-->
    	<div class="f-l w-240">
        	<div class="bor-shadow-box">
            	<h2 class="tit-name-01">材料分类</h2>
                <div class="tree-controls ztree" id="treeDemo"></div>
            </div>
           <!-- <div class="adBox-240 m-t-10">广告位</div>-->
        </div>
        <!--	左侧树	-->
        <!--	右侧筛选与数据列表	-->
        <div class="f-r w-850">
        	<!--	筛选	-->
        	<div class="bor-shadow-box">
            	<dl class="nav-category bor-no clearfix">
                	<dt>地区：</dt>
                    <dd>
                    	<ul class="nav-category-li clearfix" id="fac_city">
                        </ul>
                    </dd>
                </dl>
            </div>
            <!--	筛选	-->
            <!--	数据列表	-->
            <div class="bor-shadow-box m-t-10 clearfix">
            	<h2 class="tit-name-02"><b>供应商</b></h2>
                <!--	操作栏	-->
                <div class="date-t-option m-t-10 clearfix"> 
                	<a href="javascript:void(0);" onclick="collect(null,'1');return false;" class="btn-collection f-l">收藏</a>
                    <div class="moudle-page f-r" style="padding:0;">
                    	<div class="page-mune f-r">
                           <div class="manu f_r" id="page1">
            	
                           </div>
                        </div>
                    </div>
                </div>
                <!--	操作栏	-->
                <!--	数据显示	-->
                <div class="moudle-date-box">
                	<table>
                        <tr>
                        	<th width="20px"><input id="mycaijia_id" onclick="checkAll()" type="checkbox" value="全选" class="all_selector" /></th>
                            <th width="52%">供应商信息</th>
                            <th width="25%" class="txt-c">地区</th>
                            <th width="20%" class="txt-c">操作</th>
                        </tr>
					</table>
					<div id="listbody">
		            </div>
		            <div id="page" class="digg">
		            </div>
                </div>
                <!--	数据显示	-->
                 <a href="javascript:void(0);" style="margin-left:10px;margin-bottom:8px;" onclick="collect(null,'1');return false;" class="btn-collection f-l">收藏</a>
            </div>
            <!--	数据列表	-->
        </div>
        <!--	右侧筛选与数据列表	-->
    </div>
</div>
<!--	container	-->
#parse($zjt_tip)
<div id="tempfac_block" style="display:none;">
   <table>
       <tr id="row_@{rownum}">
            <td width="20px"><input type="checkbox" name="checkselected" value="@{eid}" class="all_selector" /></td>
            <td width="52%">
            	<p class="td-companyName"><a href="@{city|setSite}">@{name}</a></p>
                <p class="td-main-material">
                	主营材料： @{subcids|subcidUtil}
                </p>
            </td>
            <td width="25%" class="txt-c">@{province} @{city}</td>
            <td width="20%" class="txt-c">
            	<a href="@{city|setSite}" class="btn-view-quotation">查看报价</a>
            </td>
        </tr>
   </table>
</div>
#parse($footer_dir)
<script type="text/javascript" src="$common_site/js/zjt/commonJS/zhcn_db_fac.js"></script>
<script type="text/javascript" src="$common_site/js/common/zhcn_select.js"></script>
