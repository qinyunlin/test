<script type="text/javascript" src="$common_site/js/plungins/JQuery zTree v3.5.01/js/jquery.ztree.exedit-3.5.js"></script>
<script type="text/javascript" src="http://c2.zjtcn.com/js/common/SearchStore.js"></script>
<script type="text/javascript" src="$common_site/js/zjt/commonJS/zhcn_db_fac.js"></script>
<script type="text/javascript" src="$common_site/js/common/zhcn_select.js"></script>
<script type="text/javascript" src="$common_site/js/common/search_common.js"></script>
<script type="text/javascript" src="$common_site/js/zjt/fac/fac_mat_search.js"></script>
</html>