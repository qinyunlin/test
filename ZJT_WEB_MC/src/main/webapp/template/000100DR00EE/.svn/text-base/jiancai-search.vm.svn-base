<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<html>
  <head> <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />

<title>建材供应搜索 - 网材网 - 中国最大的建材信息网</title>
 <meta name="keywords" content="建材供应，建材，建筑材料，建材信息。" />
<meta name="description" content="网材网建材供应是中国建材信息最大的平台，包含涂料,门窗,水泥,建筑五金,陶瓷,管材管件,园林苗木,家具等，为建材求购方提供最齐全的建材信息" />


    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<link rel="stylesheet" type="text/css" href="$wcw_common_site/css/tradeChannel/searchList.css">
	<script type="text/javascript" src="$wcw_common_site/js/common/jyeeda.js"></script>	
	<script type="text/javascript" src="$wcw_common_site/js/common/SearchStore.js"></script>
 	<script type="text/javascript" src="$wcw_common_site/js/common/dataStore.js"></script>
	<script type="text/javascript" src="$wcw_common_site/js/common/pageTool.js"></script>
	<script type="text/javascript" src="$wcw_common_site/js/common/basic.js"></script>
	<script type="text/javascript" src="$wcw_common_site/js/common/grade_db.js"></script>
	<script type="text/javascript" src="$wcw_common_site/js/common/stuff_cid_db.js"></script>
	<script type="text/javascript" src="$wcw_common_site/js/common/stuff_cid_decode.js"></script>
	<script type="text/javascript" src="$wcw_common_site/js/common/common.js"></script>	
	<script type="text/javascript" src="$wcw_common_site/js/searchlist/jiancai_search.js"></script>	
  </head>
  
  <body>
    <!-- 头部 -->
    #parse($wcw_header)
    <div id="main">
	#parse($hypd_nav)
	<!-- 行业分类bar -->
		<div class="sub_style_box">
	<h2 id="title_div">
    	<a href="javascript:void(0);" onclick="seeAll();">全部采购分类</a>
    </h2>
     <div style="text-align:left;">
	    <ul id="cgCidList" class="clearfix">
	    	
	    </ul>
    </div>
</div>
<dl class="sel_city_box clearfix">
	<dt>地区：</dt>
    <dd>
       <div style="text-align:left;">
	        <ul id="province_div" class="nava clearfix">
	            
	         
	        </ul>
        </div>
        <div class="dim" style="display:none;" id="city_div">
           
        </div>
    </dd>
</dl>
	<div class="clearing"></div>
	<!-- 主体 -->
	<div id="main">
		<div class="main_l">
			<div class="title_banner" style="width:665px;">
				<div class="title_banner_left"></div>
				<div class="title_banner_center" style="width:659px;">
					<div class="title_icon"></div>
					<div class="title_text">建材供应</div>
				</div>
				<div class="title_banner_right"></div>
			</div>
			<div class="searchlist_container">
				
				<div class="searchlist_div">
					<div id="templist"  style="display:none;">
						<div class="searchlist_content">
							<div class="list_num">@{rownum}</div>
						
							<div class="list_img">
								<img width="80px" height="80px" src="http://ftp.zjtcn.com/@{topPhoto}" />
							</div>

							<div class="list_container">
								<div class="list_container_t">
								    <div class="type_company" style="height:22px;overflow:hidden;width:100px;">@{name}</div>			
									<div  style="height:16px;overflow:hidden;float:left;"><a  target="_blank" class="orange_a" href="$!templateUtil.getShopUrl('@{fid}')" style="font-size:14px;" title="@{ename}">@{ename}</a></div>
								
								</div>
								<div class="list_container_b">
									<div class="list_container_b" style="height:35px;overflow:hidden;">
										规格:@{spec}
									</div>
									
								</div>
							</div>
						</div>
					</div>
					
					<div id="templistobj"></div>
					<div id="page"></div>
				</div>
			</div>
			
		</div>
		
		<div class="main_r">
			<!-- 团购促销模块 -->
			#parse($wcw_tgcx)
			
			
			
			<!-- 第一个广告位 -->
			$!map.clear()
                    	$!map.put("tpath", "/20/2012/201203/20120308/")
                    	#set($list = $infoService.searchInfoContentSummarys(1, 1, $!map, true))
                    	#foreach($obj in $list)
			<div class="img_div1">
				<a rel="external nofollow" href="$!obj.getUrl()" target="_blank"><img alt="$!obj.getTitle()" width="277px" height="80px" src="$!fileUtil.getWcwFileUrl($!obj.getPicPath())" /></a>
			</div>
			#end
			<!-- 特价建材模块 -->
			#parse($wcw_tjjc)
			<!-- 第二个广告位 -->
			$!map.clear()
                    	$!map.put("tpath", "/20/2012/201203/20120309/")
                    	#set($list= $infoService.searchInfoContentSummarys(1, 1, $!map, true)) 
                    	#foreach($obj in $list)
			
			<div class="img_div2">
				<a rel="external nofollow" href="$!obj.getUrl()" target="_blank"><img  alt="$!obj.getTitle()" width="277px" height="80px" src="$!fileUtil.getWcwFileUrl($!obj.getPicPath())" /></a>
			</div>
			#end
			<!-- 行业资讯模块 -->
			#parse($wcw_hyzx)
			
			<!-- 价格行情模块 -->
			#parse($wcw_jghq)
			
			<!-- 成功故事模块 -->
			#parse($wcw_cggs)
		</div>
		<div class="clearing"></div>
	</div>
	</div>
    <!-- 尾部 -->
    #parse($wcw_footer)
  </body>
</html>
